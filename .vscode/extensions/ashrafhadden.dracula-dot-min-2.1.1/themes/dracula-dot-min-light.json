    "tokenColors": [
        {
            "scope": [
                "emphasis"
            ],
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "scope": [
                "strong"
            ],
            "settings": {
                "fontStyle": "bold"
            }
        },
        {
            "scope": [
                "header"
            ],
            "settings": {
                "foreground": "#855fbf"
            }
        },
        {
            "scope": [
                "source"
            ],
            "settings": {
                "foreground": "#282A36"
            }
        },
        {
            "scope": [
                "meta.diff",
                "meta.diff.header"
            ],
            "settings": {
                "foreground": "#7e8ec2"
            }
        },
        {
            "scope": [
                "markup.inserted"
            ],
            "settings": {
                "foreground": "#008504"
            }
        },
        {
            "scope": [
                "markup.deleted"
            ],
            "settings": {
                "foreground": "#d82f39"
            }
        },
        {
            "scope": [
                "markup.changed"
            ],
            "settings": {
                "foreground": "#a0651b"
            }
        },
        {
            "scope": [
                "invalid"
            ],
            "settings": {
                "foreground": "#d82f39",
                "fontStyle": "underline italic"
            }
        },
        {
            "scope": [
                "invalid.deprecated"
            ],
            "settings": {
                "foreground": "#282A36",
                "fontStyle": "underline italic"
            }
        },
        {
            "scope": [
                "entity.name.filename"
            ],
            "settings": {
                "foreground": "#6c7908"
